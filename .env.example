# Music Charts Configuration
# Copy this file to .env and update values as needed

# Path to MusicBee iTunes Music Library.xml
MSC_MUSICBEE_LIBRARY=E:/Musique/MusicBee/iTunes Music Library.xml

# Target year for analysis
MSC_YEAR=2026

# MusicBee playlist ID for DJ tracks
MSC_PLAYLIST_ID=4361

# Songstats API rate limit (requests per second)
MSC_SONGSTATS_RATE_LIMIT=10

# YouTube API daily quota limit
MSC_YOUTUBE_QUOTA_DAILY=10000

# Optional: Songstats API key (alternatively, store in _tokens/songstats_key.txt)
# MSC_SONGSTATS_API_KEY=your_api_key_here
